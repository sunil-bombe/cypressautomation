{
  "name": "cypressframework",
  "version": "1.0.0",
  "description": "Automation Framework for UI",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress-parallel run",
    "test": "npx cypress run",
    "headTest": "npm run test --headed",
    "chromeTest": "npm run test --browser chrome",
    "recordDashBoardTest": "npm run test --record --key fa423514-5a84-42a7-b6a0-b5e7191bba12 --reporter mochawesome",
    "GreenKartTest": "npm run test --spec \"cypress/integration/GreenKart/*\""
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "test-report/reports/json-report/results.json"
    }
  },
  "author": "Sunil Bombe",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.3",
    "cypress-parallel": "^0.14.0",
    "multiple-cucumber-html-reporter": "^3.6.2"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@cypress/browserify-preprocessor": "latest",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.8.1",
    "mongodb": "^6.3.0",
    "oracledb": "^6.3.0"
  }
}
